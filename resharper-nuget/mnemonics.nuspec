<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>mnemonics</id>
    <title>Mnemonic Live Templates</title>
    <version>1.0.0</version>
    <authors>JetBrains</authors>
    <owners>JetBrains</owners>
    <description>A set of C# and VB.NET Live Templates to quickly generate code using structured abbreviations (mnemonics), such as "c" for class, "C" for static class, "m" for method, "M" for static Method, etc.</description>
    <projectUrl>https://github.com/JetBrains/mnemonics</projectUrl>
    <iconUrl>http://resharper-plugins.jetbrains.com/Content/Images/packageReSharper.png</iconUrl>
    <copyright>Copyright &#169; 2013 JetBrains, s.r.o</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="ReSharper" version="8.0" />
    </dependencies>
    <tags>resharper templates mnemonics</tags>
  </metadata>
  <files>
    <file src="..\downloads\templates.dotSettings"
          target="ReSharper\v8.0\settings" />
  </files>
</package>
